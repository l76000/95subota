{
  "functions": {
    "api/index.js": {
      "maxDuration": 10,
      "memory": 1024
    },
    "api/clear.js": {
      "maxDuration": 10,
      "memory": 512
    }
  },
  "crons": [
    {
      "path": "/api/clear",
      "schedule": "0 2 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/",
      "destination": "/api"
    }
  ]
}
